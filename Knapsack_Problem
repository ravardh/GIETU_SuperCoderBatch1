def knapsack(weight,price,m):
    n = len(price)
    M = [[0]*(m+1) for _ in range(n+1)]
    for i in range(1,n+1):
        for c in range(1,m+1):
            if(weight[i-1]<=c):
                M[i][c] = max(M[i-1][c],M[i-1][c-weight[i-1]]+price[i-1])
    return M[n][m]      
weight = [2,3,5,7,1,4,1]
price = [10,5,15,7,6,18,3]
print(knapsack(weight,price,15))
